@{
    ViewBag.Title = "Process";
    Layout = "~/Views/Shared/_LayoutLiberty.cshtml";
}
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Welcome to the Liberty Client Portal</h1>
        </div>
        <!-- /.col-lg-8 -->
        <div class="col-lg-4">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <i class="fa fa-bell fa-fw"></i> Actions Panel
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="list-group">
                        <a href="@Url.Action("Process", new {clientName = @ViewBag.ClientName})" class="list-group-item">
                            <i class="fa fa-edit fa-fw"></i> Capture Details
                            <span class="pull-right text-muted small">
                                <em>Capture your details</em>
                            </span>
                        </a>
                    </div>
                 
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
    </div>
</div>